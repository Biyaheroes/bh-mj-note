{
	"name": "bh-mj-note",
	"version": "0.0.0",
	"description": "Biyaheroes MJML Note Component",
	"main": "note.js",
	"support": "note.support.js",
	"deploy": "note.deploy.js",
	"browser": "note.deploy.js",
	"scripts": {
		"pristine": "rsetmod clean && flxc execute ./.install.sh",
		"clear": "rsetmod clean --clear",
		"formal": "jesy apply '*.json'",
		"build:support": "webpack --config webpack.loose.js --progress",
		"build:deploy": "webpack --config webpack.compact.js --progress",
		"build:transpile": "babel note.jsx --out-file note.js --source-maps",
		"build": "npm run build:transpile && npm run build:support && npm run build:deploy",
		"build:render": "npm run build:transpile && mjml render.mjml --output render.html",
		"build:component": "webpack --config webpack.component.js --progress",
		"build:test": "npm run build:render && npm run build:component && gulp --module test --bridge",
		"base:render": "npm run build:render && scrshot take ./render.html --output='render.base' --silent",
		"base:component": "npm run build:component && scrshot take ./component.html --output='component.base' --silent",
		"base": "npm run clear && npm run base:render && npm run base:component",
		"shot:render": "scrshot take ./render.html --output='render.test' --silent",
		"shot:component": "scrshot take ./component.html --output='component.test' --silent",
		"shot": "npm run shot:render && npm run shot:component",
		"pretest": "npm run clear && npm run build:test",
		"test:client": "npm run shot && ./node_modules/.bin/wdio wdio.client.js",
		"test": "npm run test:client",
		"posttest": "npm run formal",
		"deploy": "npm run pristine && npm run build && npm run build:test && npm run test:client && npm run formal"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Biyaheroes/bh-mj-note.git"
	},
	"keywords": [
		"biyaheroes",
		"mjml",
		"note",
		"component"
	],
	"author": "Biyaheroes Developers <developers@biyaheroes.com>",
	"contributors": [
		"Robot Biyaheroes <robot@biyaheroes.com>",
		"Vinse Vinalon <vinsevinalon@gmail.com>",
		"Richeve S. Bebedor <richeve.bebedor@gmail.com>"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Biyaheroes/bh-mj-note/notes"
	},
	"homepage": "https://github.com/Biyaheroes/bh-mj-note#readme",
	"dependencies": {
		"babel-runtime": "^6.25.0",
		"bh-mj-detail": "^0.6.0",
		"doubt": "^0.34.0",
		"filpos": "^0.1.0",
		"jquery": "^3.2.1",
		"mjml": "^3.3.3",
		"mjml-column": "^3.3.3",
		"mjml-core": "^3.3.3",
		"mjml-section": "^3.3.3",
		"mjml-text": "^3.3.3",
		"mjml-wrapper": "^3.3.3",
		"nbyx": "^0.1.0",
		"parseon": "^0.10.0",
		"protype": "^0.24.0",
		"react": "^15.6.1",
		"sxty4": "^0.11.0"
	}
}
